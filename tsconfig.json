{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2020",
        "esModuleInterop": true,
        "strict": true,
        "outDir": "dist",
        "rootDir": "src",
        "noImplicitAny": true,
        "baseUrl": "src",
        "paths": {
            "*": [
                "*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}